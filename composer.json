{
    "name": "zoomyboy/php-ssh",
    "description": "Ssh WWrapper around PhpSecLib",
    "type": "library",
    "require": {
        "laravel/framework": "^6.0"
    },
	"require-dev": {
		"phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.6",
        "mockery/mockery": "^1.1"
	},
    "authors": [
    {
        "name": "Zoomyboy",
        "email": "pille@zoomyboy.de"
    }],
    "autoload": {
        "psr-4": {
            "Zoomyboy\\PhpSsh\\": "src/"
        },
		"files": [
			"helpers/functions.php"
		]
    },
	"autoload-dev": {
		"psr-4": {
			"Zoomyboy\\PhpSsh\\Tests\\": "tests/"
		}
	}
}
